-card-subtitle><b>Profile:</b> {{ item.profile }}</v-card-subtitle>\r\n    <div class=\"categories\">\r\n      <v-chip v-for=\"category in item.categories\" :key=\"category\">{{\r\n        category\r\n      }}</v-chip>\r\n    </div>\r\n    <v-card-actions>\r\n      <v-spacer></v-spacer>\r\n      <v-btn color=\"success\" :to=\"'/trainers/' + item.code\"> Editar </v-btn>\r\n      <v-btn color=\"error\" @click=\"eliminar()\"> Eliminar </v-btn>\r\n    </v-card-actions>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport { deleteTrainer } from \"../controllers/Trainer.controller\";\r\n\r\nexport default {\r\n  props: [\"item\"],\r\n  methods: {\r\n    editar() {\r\n      this.$router.push(`/trainers/${this.item.code}`);\r\n    },\r\n    eliminar() {\r\n      deleteTrainer(this.item.code)\r\n        .then(() => {\r\n          window.location.reload();\r\n        })\r\n        .catch((err) => console.error(err.response.data.message));\r\n    },\r\n  },\r\n  computed: {\r\n    imageUrl() {\r\n      return this.item.imageUrl == undefined\r\n        ? \"https://isocarp.org/app/uploads/2014/05/noimage.jpg\"\r\n        : this.item.imageUrl;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scope>\r\n.categories {\r\n  display: flex;\r\n  padding: 10px;\r\n  justify-content: center !important;\r\n  gap: 10px;\r\n}\r\n</style>"]}]}